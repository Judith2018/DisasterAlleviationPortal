@page
@model DisasterAlleviationPortal.Pages.User.RegisterModel

<form method="post">
    <input type="text" asp-for="Username" placeholder="Username" required />
    <input type="password" asp-for="Password" placeholder="Password (min 8 chars)" required minlength="8" />
    <button type="submit">Register</button>
</form>
<p>@Model.Message</p>

